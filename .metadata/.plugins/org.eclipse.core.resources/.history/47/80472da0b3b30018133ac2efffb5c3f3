
public class Dog {
	String name;
	int age;
	
	public Dog(String name, int age) {
		this.name = name;
		this.age = age;
	}
	
	public void sayName() {
		System.out.println(this.name);
	}
	
	public static void main(String[] args) {
		final Dog dog = new Dog("tommy", 19);
		final Dog dog2 = new Dog("tommy", 19); 
		System.out.println(dog.sayName());
	}
}
